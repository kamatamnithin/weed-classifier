# Copy this file to .env and fill values before running docker-compose
# ML backend API key (optional for dev). Keep secret for production.
ML_API_KEY=

# Frontend build-time API URL (set to backend host inside docker-compose)
VITE_API_URL=http://backend:8000

# Optional: Supabase and other keys used by the frontend
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
